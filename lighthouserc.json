{
    "ci": {
        "collect": {
            "numberOfRuns": 3,
            "startServerCommand": "npm run start",
            "startServerReadyPattern": "Ready in",
            "url": [
                "http://localhost:3000/"
            ]
        },
        "assert": {
            "assertions": {
                "categories:performance": [
                    "error",
                    {
                        "minScore": 0.8
                    }
                ],
                "categories:seo": [
                    "error",
                    {
                        "minScore": 1
                    }
                ]
            }
        },
        "upload": {
            "target": "temporary-public-storage"
        }
    }
}